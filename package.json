{
  "name": "dnd-14th-8-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "build-storybook": "storybook build",
    "check": "biome check .",
    "dev": "vite",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "**/*.{css,js,json,jsx,ts,tsx,html}": [
      "biome check --write --unsafe --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "axios": "^1.13.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.2",
    "react-dom": "^19.2.3",
    "react-router": "^7.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@chromatic-com/storybook": "^3.2.7",
    "@storybook/addon-essentials": "8.6.15",
    "@storybook/addon-onboarding": "8.6.15",
    "@storybook/blocks": "8.6.15",
    "@storybook/experimental-addon-test": "^8.6.15",
    "@storybook/react": "8.6.15",
    "@storybook/react-vite": "8.6.15",
    "@storybook/test": "^8.6.15",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tsconfig/node24": "^24.0.4",
    "@tsconfig/vite-react": "^7.0.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "lint-staged": "^16.2.7",
    "playwright": "^1.58.0",
    "simple-git-hooks": "^2.13.1",
    "sort-package-json": "^3.6.0",
    "storybook": "8.6.15",
    "storybook-addon-pseudo-states": "^4.0.4",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.17.0",
  "engines": {
    "node": ">=24.0.0"
  }
}
